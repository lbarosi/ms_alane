    RowBox[{"Grade", "[", 
     RowBox[{"expr", ",", "Wedge"}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"addfactor", "[", 
     RowBox[{"expr1_", ",", 
      RowBox[{"expr2_", "?", "xTensorQ"}]}], "]"}], ":=", 
    RowBox[{"expr1", "\[Wedge]", 
     RowBox[{"expr2", "[", 
      RowBox[{
       RowBox[{"-", 
        RowBox[{"First", "@", 
         RowBox[{"Select", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"IndicesOf", "[", "Free", "]"}], "[", "expr1", "]"}], ",",
            "DownIndexQ"}], "]"}]}]}], ",", 
       RowBox[{"-", 
        RowBox[{"NewIndexIn", "[", "VB", "]"}]}]}], "]"}]}]}], ";"}], 
  "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"traceindex", "[", "expr_", "]"}], ":=", 
    RowBox[{"expr", "/.", 
     RowBox[{"UpIndex", "/@", 
      RowBox[{"(", 
       RowBox[{"Rule", "@@", 
        RowBox[{
         RowBox[{"IndicesOf", "[", "Free", "]"}], "[", "expr", "]"}]}], 
       ")"}]}]}]}], ";"}], "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"rules", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"\[ScriptCapitalF]", "[", "]"}], "\[Rule]", 
       RowBox[{"CurvatureForm", "[", 
        RowBox[{"cdv", ",", "VB"}], "]"}]}], ",", 
      RowBox[{
       RowBox[{"\[ScriptCapitalA]", "[", "]"}], "\[Rule]", 
