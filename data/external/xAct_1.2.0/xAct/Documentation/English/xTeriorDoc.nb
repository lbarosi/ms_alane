 RowBox[{"DefManifold", "[", 
  RowBox[{"M", ",", "dim", ",", 
   RowBox[{"{", 
    RowBox[{
    "a", ",", "b", ",", "c", ",", "d", ",", "e", ",", "f", ",", "g", ",", 
     "h"}], "}"}]}], "]"}]], "Input",
 CellLabel->"In[11]:=",ExpressionUUID->"62669147-3273-4355-a92c-9b17ab93a78b"],

Cell[BoxData[
 RowBox[{"DefDiffForm", "[", 
  RowBox[{
